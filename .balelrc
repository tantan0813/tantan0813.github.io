{
     "presets": ["react","es2015-loose","stage-0"],
     "plugins": [
      	"syntax-export-extensions",
      	"babel-plugin-add-module-exports",
        "react-html-attrs",
        "transform-class-properties",
        "transform-decorators-legacy"
      ],
      "compact": false,
	  "ignore":[
        "jquery.js",
        "jquery.min.js",
        "angular.js",
        "angular.min.js",
        "bootstrap.js",
        "bootstrap.min.js"
    ]
}